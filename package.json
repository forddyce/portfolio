{
  "name": "gulp-starter",
  "version": "0.0.1",
  "description": "Laravel gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/forddyce/gulp-starter.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "private": true,
  "devDependencies": {
    "browser-sync": "~2.6.9",
    "browserify": "^10.0.0",
    "browserify-shim": "^3.8.6",
    "coffeeify": "~1.1.0",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.2.0",
    "gulp-changed": "^1.2.1",
    "gulp-coffeelint": "^0.4.0",
    "gulp-csscomb": "^3.0.3",
    "gulp-filesize": "0.0.6",
    "gulp-iconfont": "^1.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-minify-css": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "~1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^1.3.3",
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "lazyload": "1.8.4",
    "lodash": "^3.8.0",
    "pretty-hrtime": "~1.0.0",
    "psi": "^1.0.6",
    "require-dir": "^0.3.0",
    "run-sequence": "~1.1.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "^3.2.1"
  },
  "dependencies": {
    "del": "^1.1.1",
    "jquery": "~2.1.4"
  }
}
