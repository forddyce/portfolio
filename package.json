{
  "name": "gulp-starter",
  "version": "0.0.1",
  "description": "Laravel gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/forddyce/gulp-starter.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "private": true,
  "devDependencies": {
    "browser-sync": "~2.8.0",
    "browserify": "^11.0.0",
    "browserify-shim": "^3.8.10",
    "coffeeify": "~1.1.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-changed": "^1.2.1",
    "gulp-coffeelint": "^0.5.0",
    "gulp-concat": "~2.6",
    "gulp-csscomb": "^3.0.6",
    "gulp-filesize": "0.0.6",
    "gulp-iconfont": "^3.0.3",
    "gulp-imagemin": "^2.3.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "~1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.4",
    "gulp-size": "^1.2.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "lazyload": "1.8.4",
    "lodash": "^3.10.0",
    "pretty-hrtime": "~1.0.0",
    "psi": "^1.0.6",
    "require-dir": "^0.3.0",
    "run-sequence": "~1.1.2",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "^3.3.0"
  },
  "dependencies": {
    "del": "^1.2.0",
    "jquery": "~2.1.4",
    "pngquant-bin": "0.1.6",
    "gifsicle": "0.1.4",
    "jpegtran-bin": "0.2.3",
    "optipng-bin": "0.3.1",
    "pngquant-bin": "0.1.6"
  }
}
